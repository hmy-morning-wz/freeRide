   position: absolute;
   /* demo3 linear 0.8s infinite; alternate*/
   animation: demo7 linear 1.3s   infinite ;
   -webkit-animation: demo7 linear 1.3s   infinite ;
 }

 @keyframes demo5 {
   0% {
     transform: scale(1,1) translateX(0px);
     -webkit-transform: scale(1,1) translateX(0px);
   }

   12.5% {
     transform: translateX(20px) scale(0.8,0.8) ;
     -webkit-transform: translateX(20px) scale(0.8,0.8) ;
   }
   
   25% {
     transform: scale(1,1) translateX(40px);
     -webkit-transform: scale(1,1) translateX(40px);
   }
   
   37.5% {
     transform: scale(1.2,1.2) translateX(60px);
     -webkit-transform: scale(1.2,1.2) translateX(60px);
   }
   
   
   50% {
     transform: scale(1,1) translateX(80px);
     -webkit-transform: scale(1,1) translateX(80px);
   }

   62.5% {
     transform: scale(0.8,0.8) translateX(60px);
     -webkit-transform: scale(0.8,0.8) translateX(60px);
   }

  75% {
     transform: scale(1,1) translateX(40px);
     -webkit-transform: scale(1,1) translateX(40px);
   }

   87.5% {
     transform: scale(1.2,1.2) translateX(20px);
     -webkit-transform: scale(1.2,1.2) translateX(20px);
   }
  
   100% {
     transform: translateX(0px) scale(1,1) ;
     -webkit-transform:translateX(0px) scale(1,1) ;
   }
 }


 .guidance-modal-ball-show {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10000;
}

.guidance-modal-ball-hide {
  display: none;
}

.guidance-modal-mask-ball, .am-modal-document {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.guidance-modal-mask-ball {
  background-color: rgba(0, 0, 0, 0);
  text-align: center;
}
 
 
 #loading5 {
   width: 100px;
   height: 100px;
   /* border-bottom: 1px solid #68b2ce; */
 }

 .demo5 {
   width: 15px;
   height: 15px;
   border-radius: 10px;
   background: #FF4C4B;
   position: absolute;
   /* demo3 linear 0.8s infinite; alternate*/
   animation: demo5 linear 1.3s   infinite ;
   -webkit-animation: demo5 linear 1.3s   infinite ;
 }
 .demo6 {
   width: 15px;
   height: 15px;
   position: absolute;
   border-radius: 10px;
   background: #3E8CFF;
   /* demo3 linear 0.8s infinite; */
   animation: demo6 linear 1.3s   infinite ;
   -webkit-animation: demo6 linear 1.3s   infinite ;

.popup-rule-show {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10001;
}
.popup-rule-hide {
  display: none;
}
.popup-rule-mask,
.popup-rule-document {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.popup-rule-mask {
  background-color: rgba(0, 0, 0, 0.75);
}
.popup-rule-document {
  display: flex;
  align-items: center;
  justify-content: center;
}
.popup-rule {
 /* background-color: #fff;*/
  border-radius: 20rpx;
  width:542rpx;
  background: white;
  position: relative;
}
.popup-rule .title {
  margin-top: 34rpx;
  text-align: center;
  width:100%;
  height:48rpx;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(66,82,110,1);
  line-height:48rpx;
}

.popup-rule .body {
  margin-top: 20rpx;
  overflow: hidden;
  margin-left:64rpx; 
  width:414rpx;
  min-height: 520rpx;
  max-height: 520rpx;
  padding: 0 0px;
  text-align: left;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(122,134,154,1);
  line-height:40rpx;
  margin-bottom: 20rpx;
  white-space: pre-line;
}
.popup-rule  .body scroll-view {
  height: 520rpx;
  width: 100%;
  
}


.popup-rule-close {
  position: absolute;
  display: block;
  left: 50%;
  transform : translate(-50%,0);
  bottom: -50px;
  height: 48px;
  width: 48px;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 16px 16px;
}

.popup-rule-white {
  background: url("https://gw.alipayobjects.com/zos/rmsportal/fdmdsySxNBrpcVBVGEUM.png") center no-repeat;;
  background-size: 16px 16px;
}
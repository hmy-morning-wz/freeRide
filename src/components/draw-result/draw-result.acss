}
@keyframes starflash {
  0% {
    opacity: 0;
    transform: scale(0.1);
  }
  50% {
    opacity: 1;
    transform: scale(2);
  }
  100% {
    opacity: 0;
    transform: scale(0);
  }
}
.wave {
  position: absolute;
  width: 1500rpx;
  height: 1500rpx;
  text-align: center;
}
.wave .circle {
  position: absolute;
  border-radius: 50%;
  opacity: 0;
}
/* 波纹效果 */
.wave.ripple .circle {
  width: calc(100% - 6px);
  /* 减去边框的大小 */
  height: calc(100% - 6px);
  /* 减去边框的大小 */
  /* border: 10rpx solid #fff;;*/
  border-color: white;
  /* box-shadow: 0px 0px 10rpx white;*/
  background: url("https://images.allcitygo.com/miniapp/202005/gq.png?x-oss-process=image/format,webp") center no-repeat;
  background-size: 100% 100%;
}
.wave.ripple .circle:first-child {
  animation: circle-opacity 0.75s 1;
  animation-delay: 0.3s;
}
.wave.ripple .circle:nth-child(2) {
  animation: circle-opacity 0.75s 1;
  animation-delay: 0.6s;
}
.wave.ripple .circle:nth-child(3) {
  animation: circle-opacity 0.75s 1;
  animation-delay: 0.9s;
}
@keyframes circle-opacity {
  0% {
    opacity: 0.5;
    transform: scale(0.5);
  }
  100% {
    opacity: 0;
    transform: scale(1);
  }
}

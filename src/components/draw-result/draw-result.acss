.am-modal-show {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1101;
  perspective: 1500rpx;
}
.am-modal-hide {
  display: none;
}
.am-modal-mask,
.am-modal-document {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.am-modal-mask {
  background-color: rgba(0, 0, 0, 0.75);
}
.am-modal-document {
  display: flex;
  align-items: center;
  justify-content: center;
}
.am-modal {
  /* background-color: #fff;*/
  border-radius: 20rpx;
  width: 700rpx;
  height: 695rpx;
  position: relative;
  /*background: url("https://images.allcitygo.com/miniapp/202005/redbg.png?x-oss-process=image/format,webp")
    center no-repeat;
  background-size: 609rpx 100%;*/
}
.am-modal-header {
  font-size: 18px;
  line-height: 21px;
  text-align: center;
  color: #333;
  font-weight: 600;
  padding-top: 22px;
}
.am-modal-body {
  margin-top: 0px;
  overflow: hidden;
  width: 100%;
  padding: 0 0px;
  line-height: 20px;
  text-align: center;
  color: #333;
  font-size: 14px;
}
.am-modal-footer {
  position: absolute;
  margin-top: 0px;
  left: 50%;
  transform: translate(-50%, 0);
  width: 480rpx;
  top: 546rpx;
  height: 80rpx;
  text-align: center;
  background: linear-gradient(180deg, #ffc000 0%, #ea9b00 100%);
  box-shadow: 0px 8rpx 16rpx 0px #ffc000;
  border-radius: 40rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 600;
  color: #ffffff;
  line-height: 80rpx;
}
.am-modal-footer text {
  color: #db0938;
}
.am-modal-close {
  position: absolute;
  display: block;
  left: 50%;
  transform: translate(-50%, 0);
  bottom: -50px;
  height: 48px;
  width: 48px;
  background: url("https://img.alicdn.com/tfs/TB1ZvjLAGL7gK0jSZFBXXXZZpXa-40-40.png") center no-repeat;
  background-size: 38rpx 38rpx;
}
.am-modal-close-active,
.am-modal-footer-active {
  background-color: rgba(37, 39, 40, 0.05);
}

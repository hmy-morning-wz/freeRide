@keyframes flip0 {
  0% {
    transform: rotateY(270deg) scale(0.5);
    width: 100%;
  }
  25% {
    transform: rotateY(-90deg) scale(1);
    width: 100%;
  }
  50% {
    transform: rotateY(45deg);
    width: 100%;
  }
  75% {
    transform: rotateY(-30deg);
    width: 100%;
  }
  100% {
    transform: rotateY(0deg);
  }
}
@keyframes flip1 {
  from {
    transform: rotateY(270deg) scale(0.5);
    width: 100%;
  }
  to {
    transform: rotateY(0deg) scale(1);
  }
}
@keyframes flip2 {
  from {
    transform: rotateY(-60deg);
    width: 100%;
  }
  to {
    transform: rotateY(30deg);
  }
}
@keyframes flip3 {
  from {
    transform: rotateY(30deg);
    width: 100%;
  }
  to {
    transform: rotateY(-15deg);
  }
}
@keyframes flip4 {
  from {
    transform: rotateY(-15deg);
    width: 100%;
  }
  to {
    transform: rotateY(0);
  }
}
.animate {
  animation: flip1 0.7s cubic-bezier(1, 1.65, 0.8, 0.79) 0s;
}
.star-box {
  position: absolute;
